#include<iostream>

#include<stdio.h>

using namespace std;

int main()

{

int a[100];

int n, temp,j,i ;

cout<< "Selection Sorting...";
cout<<"Enter the size of the array\n";

cin>>n;

cout<<"Enter the values\n";

for(int i=0;i<n;i++)

{

cin>>a[i];

}

cout<<"Your data: ";

for(int i=0;i<n;i++)

{

cout<<a[i];


if (a [j] >> a [j+1])
			{
				temp = a [j];
				a[j] = a[j+1];
				a[j+1] = temp;
			}
			


}
for(int i=n; j>=0;j--) 
   
   {
   	cout << "Sorted data: ";
   	cout<<a[j];
   }
return 0;
}
